{
  "name": "snake-api",
  "version": "0.1.0",
  "description": "A simple Node app providing API for Snake Game Leaderboard.",
  "main": "dist/server.js",
  "scripts": {
    "watch-compile": "parcel src/server.ts --target=node",
    "watch-run": "nodemon dist/server.js",
    "build": "parcel build src/server.ts --target=node",
    "launch": "node dist/server.js",
    "start": "npm run build && npm run launch"
  },
  "dependencies": {
    "@types/better-sqlite3": "^3.1.3",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.11.1",
    "better-sqlite3": "^4.1.1",
    "express": "^4.16.3",
    "parcel": "^1.8.1",
    "parcel-bundler": "^1.8.1",
    "parcel-plugin-typescript": "^0.7.1"
  },
  "repository": {
    "url": "https://github.com/hasparus/snake-game-api"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express",
    "snake",
    "api"
  ],
  "devDependencies": {
    "nodemon": "^1.17.5",
    "prettier": "^1.11.1",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.3"
  }
}